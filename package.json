    "coverage": "jest --coverage"
  },
  "author": "Jen Wachter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/johnshopkins/tag-bump.git"
  },
  "homepage": "https://github.com/johnshopkins/tag-bump",
  "bugs": {
    "url": "https://github.com/johnshopkins/tag-bump/issues"
  },
  "dependencies": {
